{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"sourceMap": true,
		"strict": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowUmdGlobalAccess": true
    },
    "include": ["src"],
	"exclude": ["node_modules"]
}
